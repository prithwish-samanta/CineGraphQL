type Query {
    movie(id: ID): Movie
    movies:[Movie]
}

type Movie {
    movieId: ID!
    title: String!
    releaseDate: String
    duration: Int
    plotSummary: String
    posterUrl: String
    trailerUrl: String
    genres: [Genre]
    actors: [Actor]
}

type Genre {
    genreId: ID!
    name: String!
}

type Actor {
    actorId: ID!
    name: String!
    birthDate: String
    biography: String
    profilePictureUrl: String
}